{
  "extends": ["eslint:all", "airbnb-base", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "globals": {
    "console": true,
    "Promise": true,
    "fetch": true
  },
  "rules": {
    "arrow-body-style": 0,
    "no-console": 0,
    "no-underscore-dangle": 0,
    "no-use-before-define": 0,

    "no-param-reassign": 1,
    "no-unused-expressions": 1,
    "no-unused-vars": 1,
    "prefer-promise-reject-errors": 1
  }
}
